{
  "name": "oidc-jwt-client",
  "version": "1.0.0",
  "description": "Fetch JWTs for API access from oidc-jwt-provider",
  "sideEffects": false,
  "source": "src/index.ts",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "Sebastiaan Besselsen <s.besselsen@sdu.nl>",
  "license": "MIT",
  "contributors": [
    "Lennard Westerveld <L.Westerveld@sdu.nl>",
    "Daphne Smit <d.smit@sdu.nl>"
  ],
  "scripts": {
    "lint": "eslint src --ext .ts,.tsx,.js,.jsx",
    "lint-fix": "eslint --fix src --ext .ts,.tsx,.js,.jsx",
    "type-check": "tsc --noEmit",
    "build:types": "ttsc --project ./tsconfig.json -d --emitDeclarationOnly --noEmit false --outDir dist/types --declarationDir dist/types",
    "build:rollup": "rollup -c",
    "build": "npm run clean && npm run build:rollup && npm run build:types",
    "clean": "rimraf dist; rollup -c; rimraf \"dist/**/*.stories.d.ts\"; rimraf \"dist/stories\"",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "prepublishOnly": "npm run build",
    "deploy": "semantic-release"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-essentials": "^6.1.21",
    "@storybook/addon-links": "^6.1.21",
    "@storybook/react": "^6.1.21",
    "@types/react": "^17.0.3",
    "babel-loader": "^8.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.22.0",
    "husky": "^5.2.0",
    "prettier": "^2.2.1",
    "query-string": "^7.0.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-is": "^17.0.2",
    "rimraf": "^3.0.2",
    "sdu-react-scripts": "^3.2.3",
    "semantic-release": "^17.4.2",
    "ttypescript": "^1.5.12",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "react-use": "^17.2.1",
    "zustand": "^3.3.3"
  },
  "peerDependencies": {
    "react": ">=16"
  },
  "release": {
    "branches": [
      "master",
      {
        "name": "develop",
        "prerelease": true
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "**/*.(js|ts|tsx)": [
      "eslint --fix"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx lint-staged",
      "pre-push": "npm run type-check"
    }
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
